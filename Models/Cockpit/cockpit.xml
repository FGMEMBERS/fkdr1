<?xml version="1.0"?>

<PropertyList>

 <path>cockpit.ac</path>

 <effect>
  <inherits-from>Aircraft/fkdr1/Models/Effects/reflection</inherits-from>
  <object-name>metal</object-name>
 </effect>

 <model>
  <name>magnetos</name>
  <path>Aircraft/fkdr1/Models/Cockpit/entzundung.xml</path>
  <offsets>
   <x-m>1.05</x-m>
   <y-m>-0.144</y-m>
   <z-m>-0.0455</z-m>
  </offsets>
 </model>

 <model>
  <name>fuel</name>
  <path>Aircraft/fkdr1/Models/Cockpit/benzin.xml</path>
  <offsets>
   <x-m>0.04</x-m>
   <y-m>0.0</y-m>
   <z-m>0.145</z-m>
  </offsets>
 </model>

 <model>
  <name>height</name>
  <path>Aircraft/fkdr1/Models/Cockpit/hohe.xml</path>
  <offsets>
   <x-m>0.85</x-m>
   <y-m>0.253</y-m>
   <z-m>-0.095</z-m>
   <pitch-deg>-12</pitch-deg>
   <heading-deg>-47.5</heading-deg>
   <roll-deg>14.0</roll-deg>
  </offsets>
 </model>

 <model>
  <name>rpm</name>
  <path>Aircraft/fkdr1/Models/Cockpit/umlaufe.xml</path>
  <offsets>
   <x-m>0.775</x-m>
   <y-m>-0.028</y-m>
   <z-m>-0.215</z-m>
   <pitch-deg>-20</pitch-deg>
  </offsets>
 </model>

 <model>
  <name>watch</name>
  <path>Aircraft/fkdr1/Models/Cockpit/uhr.xml</path>
  <offsets>
   <x-m>0.83</x-m>
   <y-m>0.19</y-m>
   <z-m>-0.018</z-m>
   <pitch-deg>-20</pitch-deg>
   <heading-deg>-10</heading-deg>
  </offsets>
 </model>

 <model>
  <name>compass</name>
  <path>Aircraft/fkdr1/Models/Cockpit/kompass.xml</path>
  <offsets>
   <x-m>0.856</x-m>
   <y-m>0.29175</y-m>
   <z-m>-0.470</z-m>
   <heading-deg>-90</heading-deg>
  </offsets>
 </model>

 <model>
  <name>spedometer</name>
  <path>Aircraft/fkdr1/Models/Cockpit/geschwindigkeit.xml</path>
  <offsets>
   <x-m>0.10</x-m>
   <y-m>-2.43</y-m>
   <z-m>0.3</z-m>
   <heading-deg>60</heading-deg>
  </offsets>
 </model>

 <animation>
  <type>rotate</type>
  <object-name>throttle</object-name>
  <object-name>throttlePusher</object-name>
  <property>controls/engines/engine/mixture</property>
  <factor>-65</factor>
  <center>
   <x-m>0.92</x-m>
   <y-m>0</y-m>
   <z-m>-0.24</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 <animation>
  <type>knob</type>
  <object-name>throttle</object-name>
  <value>0.0</value>
  <action>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine/mixture</property>
    <factor>0.1</factor>
   </binding>
  </action>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>throttlePusher</object-name>
  <property>controls/engines/engine/mixture</property>
  <factor>65</factor>
  <center>
   <x-m>0.87</x-m>
   <y-m>0</y-m>
   <z-m>-0.3</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>


 <animation>
  <type>translate</type>
  <object-name>fuel_tap</object-name>
  <property>controls/engines/engine/fuel-pump</property>
  <offset-m>-0.01</offset-m>
  <factor>-0.015</factor>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>
 <animation>
  <type>knob</type>
  <object-name>fuel_tap</object-name>
  <value>0.0</value>
  <action>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine/fuel-pump</property>
    <factor>1</factor>
   </binding>
  </action>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>column</object-name>
  <property>controls/flight/elevator</property>
  <factor>-10</factor>
  <center>
   <x-m>0.86</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.6</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>column</object-name>
  <property>controls/flight/aileron</property>
  <factor>-12</factor>
  <center>
   <x-m>0.86</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.6</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

