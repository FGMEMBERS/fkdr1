<?xml version="1.0"?>

<PropertyList>

 <path>cockpit.ac</path>

 <model>
  <name>fuel</name>
  <path>Aircraft/fkdr1/Models/Cockpit/benzin.xml</path>
  <offsets>
   <x-m>0.04</x-m>
   <y-m>0.0</y-m>
   <z-m>0.145</z-m>
  </offsets>
 </model>

 <model>
  <name>height</name>
  <path>Aircraft/fkdr1/Models/Cockpit/hohe.xml</path>
  <offsets>
   <x-m>0.85</x-m>
   <y-m>0.253</y-m>
   <z-m>-0.115</z-m>
   <pitch-deg>-12</pitch-deg>
   <heading-deg>-47.5</heading-deg>
   <roll-deg>0.0</roll-deg>
  </offsets>
 </model>

 <model>
  <name>rpm</name>
  <path>Aircraft/fkdr1/Models/Cockpit/umlaufe.xml</path>
  <offsets>
   <x-m>0.775</x-m>
   <y-m>-0.028</y-m>
   <z-m>-0.215</z-m>
   <pitch-deg>-20</pitch-deg>
  </offsets>
 </model>

 <model>
  <name>watch</name>
  <path>Aircraft/fkdr1/Models/Cockpit/uhr.xml</path>
  <offsets>
   <x-m>0.83</x-m>
   <y-m>0.19</y-m>
   <z-m>-0.018</z-m>
   <pitch-deg>-20</pitch-deg>
   <heading-deg>-10</heading-deg>
  </offsets>
 </model>

 <model>
  <name>compass</name>
  <path>Aircraft/fkdr1/Models/Cockpit/kompass.xml</path>
  <offsets>
   <x-m>0.856</x-m>
   <y-m>0.29175</y-m>
   <z-m>-0.470</z-m>
   <heading-deg>-90</heading-deg>
  </offsets>
 </model>

 <animation>
  <type>rotate</type>
  <object-name>throttle</object-name>
  <object-name>throttlePusher</object-name>
  <property>controls/engines/engine[1]/throttle</property>
  <factor>-65</factor>
  <center>
   <x-m>0.92</x-m>
   <y-m>0</y-m>
   <z-m>-0.24</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>throttlePusher</object-name>
  <property>controls/engines/engine[1]/throttle</property>
  <factor>65</factor>
  <center>
   <x-m>0.89</x-m>
   <y-m>0</y-m>
   <z-m>-0.28</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>steerColom</object-name>
  <object-name>yokeHinge</object-name>
  <object-name>yokeGrip</object-name>
  <property>controls/flight/elevator</property>
  <factor>-7</factor>
  <center>
   <x-m>0.82</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.56</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>yokeHinge</object-name>
  <object-name>yokeGrip</object-name>
  <property>controls/flight/elevator</property>
  <factor>7</factor>
  <center>
   <x-m>0.82</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.14</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>yokeGrip</object-name>
  <property>controls/flight/aileron</property>
  <factor>-20</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.145</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>rudderPedals</object-name>
  <object-name>leftRudderWire</object-name>
  <object-name>rightRudderWire</object-name>
  <property>controls/flight/rudder</property>
  <factor>-12</factor>
  <center>
   <x-m>0.66</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>leftRudderWire</object-name>
  <property>controls/flight/rudder</property>
  <factor>12</factor>
  <center>
   <x-m>0.68</x-m>
   <y-m>-0.12</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>rightRudderWire</object-name>
  <property>controls/flight/rudder</property>
  <factor>12</factor>
  <center>
   <x-m>0.68</x-m>
   <y-m>0.12</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

</PropertyList>
