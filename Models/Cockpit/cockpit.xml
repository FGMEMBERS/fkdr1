 <animation>
  <type>rotate</type>
  <object-name>rudderPedals</object-name>
  <object-name>leftRudderWire</object-name>
  <object-name>rightRudderWire</object-name>
  <property>controls/flight/rudder</property>
  <factor>-12</factor>
  <center>
   <x-m>0.66</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>leftRudderWire</object-name>
  <property>controls/flight/rudder</property>
  <factor>12</factor>
  <center>
   <x-m>0.68</x-m>
   <y-m>-0.12</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>rightRudderWire</object-name>
  <property>controls/flight/rudder</property>
  <factor>12</factor>
  <center>
   <x-m>0.68</x-m>
   <y-m>0.12</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

</PropertyList>
